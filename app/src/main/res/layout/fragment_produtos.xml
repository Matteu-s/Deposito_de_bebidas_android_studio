<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.produtos.ProdutosFragment">

    <Button
        android:id="@+id/btnCachaca"
        android:layout_width="@dimen/TamanhoCategoria_width"
        android:layout_height="@dimen/TamanhoCategoria_height"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="39dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/bebibasalcoolicas"
        app:layout_constraintBottom_toTopOf="@+id/txtViewCachaca"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/txtViewCachaca"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="25dp"
        android:text="CachaÃ§a"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/btnTequila"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCachaca"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/btnWhiskey"
        android:layout_width="@dimen/TamanhoCategoria_width"
        android:layout_height="@dimen/TamanhoCategoria_height"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/whiskey"
        app:layout_constraintBottom_toTopOf="@+id/txtViewWhiskey"
        app:layout_constraintEnd_toStartOf="@+id/btnVodka"
        app:layout_constraintStart_toEndOf="@+id/btnCachaca"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/txtViewWhiskey"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginStart="34dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="35dp"
        android:layout_marginRight="35dp"
        android:layout_marginBottom="26dp"
        android:text="Whiskey"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/btnPetisco"
        app:layout_constraintEnd_toStartOf="@+id/txtViewVodka"
        app:layout_constraintStart_toEndOf="@+id/txtViewCachaca"
        app:layout_constraintTop_toBottomOf="@+id/btnWhiskey"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/btnVodka"
        android:layout_width="@dimen/TamanhoCategoria_width"
        android:layout_height="@dimen/TamanhoCategoria_height"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/vodka"
        app:layout_constraintBottom_toTopOf="@+id/txtViewVodka"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/txtViewVodka"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="57dp"
        android:layout_marginRight="57dp"
        android:layout_marginBottom="25dp"
        android:text="Vodka"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/btnRefrigerante"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnVodka"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnTequila"
        android:layout_width="@dimen/TamanhoCategoria_width"
        android:layout_height="@dimen/TamanhoCategoria_height"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/tequila"
        app:layout_constraintBottom_toTopOf="@+id/txtViewTequila"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <TextView
        android:id="@+id/txtViewTequila"
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginBottom="370dp"
        android:text="Tequila"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button

        android:id="@+id/btnPetisco"
        android:layout_width="@dimen/TamanhoCategoria_width"
        android:layout_height="@dimen/TamanhoCategoria_height"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/amendoim"
        app:layout_constraintEnd_toStartOf="@+id/btnRefrigerante"
        app:layout_constraintStart_toEndOf="@+id/btnTequila"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteY="246dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="373dp"
        android:text="Refrigerantes"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnRefrigerante"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="271dp" />

    <TextView
        android:layout_width="80dp"
        android:layout_height="20dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:layout_marginBottom="373dp"
        android:text="Petiscos"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toEndOf="@+id/txtViewTequila"
        app:layout_constraintTop_toBottomOf="@+id/btnPetisco"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnRefrigerante"
        android:layout_width="@dimen/TamanhoCategoria_width"
        android:layout_height="@dimen/TamanhoCategoria_height"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        android:background="@drawable/cc"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="246dp" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Boteco de Esquina"
        android:textColor="#ffae00"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />


    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="258dp"
        android:layout_height="37dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/nav_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.105" />



    <Button
        android:id="@+id/button"
        android:layout_width="111dp"
        android:layout_height="38dp"
        android:text="Pesquisar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.046" />

</androidx.constraintlayout.widget.ConstraintLayout>